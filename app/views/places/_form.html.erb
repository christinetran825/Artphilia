<%= form_for @place do |f| %>
  <%= render 'shared/form_errors', target: @place %>
  <div class="field">
    <%= f.label "Name" %><br>
    <%= f.text_field :name %>
  </div><br>
  <div class="field">
    <%= f.label "Website (enter None for No Website)" %><br>
    <%= f.text_field :website %>
  </div><br>
  <div class="field">
    <%= f.label "Seasons (When do they open?)" %><br>
    <%= f.text_field :season %>
  </div><br>
  <div class="field">
    <%= f.label "Rating (enter 1-5)" %><br>
    <%= f.text_field :rating %>
  </div><br>
  <div class="field">
    <!-- #show checkmarks for gallery, craft fairs, farmers market, create a new place -->
    <%= f.label "Event (type)"%><br>
    <%= f.text_field :event_type %>
  </div><br>
    <%= f.submit class: 'btn btn-success' %>
<% end %>
<br>
